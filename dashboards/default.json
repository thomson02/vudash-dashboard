{
  "name": "simple-dashboard",
  "layout": {
    "columns": 5,
    "rows": 4
  },
  "widgets": [
    {
      "position": {
        "x": 1,
        "y": 0,
        "w": 3,
        "h": 1
      },
      "widget": "vudash-widget-time"
    },
    { "position": {"x": 1, "y": 2, "w": 1, "h": 1},
      "widget": "vudash-widget-ci",
      "options": {
        "provider": "travis",
        "repo": "vudash-widget-ci",
        "user": "vudash"
      }
    },
    {
      "position": {
        "x": 1,
        "y": 1,
        "w": 3,
        "h": 1
      },
      "widget": "vudash-widget-image",
      "options":{
        "imageUrl": "https://images.google.co.uk/images/branding/googlelogo/1x/googlelogo_color_272x92dp.png",
        "imageSize": "50%"
      }
    }
  ]
}
